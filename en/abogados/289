    //FILTROS
    $('#selectAreas').change(function() {
        var search = $('input[name="cv"]').val(),
            area = $('#selectAreas').val(),
            industria = $('#selectIndustrias').val();
        $('input[name="a"]').attr('value',area);
        $('input[name="i"]').attr('value',industria);
        window.location.replace("https://www.claro.cl/en/abogados/?cv="+search+"&a="+area+"&i="+industria+"");
    });
    $('#selectIndustrias').change(function() {
        var search = $('input[name="cv"]').val(),
